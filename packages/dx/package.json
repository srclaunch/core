{
  "name": "@srclaunch/dx",
  "description": "Quality of life configuration and tooling for development environments.",
  "author": "Steven Bennett <steven@srclaunch.com>",
  "license": "MIT",
  "version": "3.19.26",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "release": "semantic-release"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "type": "module",
  "files": [
    ".commitlintrc.mjs",
    ".commitlintrc.cjs",
    ".eslintrc.browser.mjs",
    ".eslintrc.browser.cjs",
    ".eslintrc.mjs",
    ".eslintrc.cjs",
    ".eslintrc.node.mjs",
    ".eslintrc.node.cjs",
    ".prettierrc.mjs",
    ".prettierrc.cjs",
    ".stylelintrc.mjs",
    ".stylelintrc.cjs",
    ".versionrc.mjs",
    ".versionrc.cjs",
    "ava.config.mjs",
    "ava.config.cjs",
    "jest.config.mjs",
    "jest.config.cjs",
    "tsconfig.json",
    "tsconfig.browser.json",
    "tsconfig.node.json"
  ],
  "exports": {
    "./.commitlintrc": {
      "import": "./.commitlintrc.mjs",
      "require": "./.commitlintrc.cjs"
    },
    "./.eslintrc": {
      "import": "./.eslintrc.mjs",
      "require": "./.eslintrc.cjs"
    },
    "./.eslintrc.browser": {
      "import": "./.eslintrc.browser.mjs",
      "require": "./.eslintrc.browser.cjs"
    },
    "./.eslintrc.node": {
      "import": "./.eslintrc.node.mjs",
      "require": "./.eslintrc.node.cjs"
    },
    "./.prettierrc": {
      "import": "./.prettierrc.mjs",
      "require": "./.prettierrc.cjs"
    },
    "./.releaserc": {
      "import": "./.releaserc.mjs",
      "require": "./.releaserc.cjs"
    },
    "./.stylelintrc": {
      "import": "./.stylelintrc.mjs",
      "require": "./.stylelintrc.cjs"
    },
    "./.versionrc": {
      "import": "./.versionrc.mjs",
      "require": "./.versionrc.cjs"
    },
    "./ava.config": {
      "import": "./ava.config.mjs",
      "require": "./ava.config.cjs"
    },
    "./jest.config": {
      "import": "./jest.config.mjs",
      "require": "./jest.config.cjs"
    },
    "./tsconfig.json": {
      "import": "./tsconfig.json",
      "require": "./tsconfig.json"
    },
    "./tsconfig.browser.json": {
      "import": "./tsconfig.browser.json",
      "require": "./tsconfig.browser.json"
    },
    "./tsconfig.node.json": {
      "import": "./tsconfig.node.json",
      "require": "./tsconfig.node.json"
    }
  }
}
