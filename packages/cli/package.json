{
  "name": "@srclaunch/cli",
  "description": "Command line tools used by SrcLaunch applications and services",
  "author": "Steven Bennett <steven@srclaunch.com>",
  "license": "MIT",
  "version": "3.19.23",
  "engines": {
    "node": ">=16"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "bin": {
    "srclaunch": "dist/index.js"
  },
  "type": "module",
  "files": [
    "package.json",
    "dist"
  ],
  "module": "./dist/index.js",
  "scripts": {
    "build:es": "esbuild --external:'@rauschma/stringio' --external:'@yarnpkg/core' --external:'@srclaunch/codegen' --external:chokidar --external:mocha --external:'vite-plugin-pwa' --external:c8 --external:diff --external:hygen --external:'extract-react-types' --external:'react-docgen-typescript' --external:ink --external:'fs-extra' --external:'gh-pages' --external:globby --external:'unconfig' --external:semver --external:esbuild --external:'semantic-release-commit-filter' --external:jest --external:'js-yaml' --external:'react' --banner:js=\"import React from 'react'\" --external:'latest-version' --external:meow --external:ora --external:picocolors --external:prompts  --external:'semantic-release' --external:'semantic-release-monorepo' --external:'simple-git' --external:'standard-version' --external:tape --external:'ts-node' --external:typescript --external:vite --external:'@vitejs/plugin-react' --external:'update-notifier' --external:vitest --bundle --minify --sourcemap --outfile=dist/index.js --format=esm --target=esnext --define:'navigator={}' --platform=node src/index.ts",
    "build": "pnpm install && rm -fr ./dist && pnpm build:es && tsc --build",
    "dev": "NODE_ENV=dev && node --loader ts-node/esm --experimental-specifier-resolution=node ./src/index.ts",
    "git:push": "git push --follow-tags origin main",
    "qr": "pnpm build && git add . && git commit -m \"Quick release\" && pnpm release",
    "release": "srclaunch release",
    "release:local": "srclaunch release --local",
    "start": "node --experimental-modules --experimental-specifier-resolution=node dist/index.js",
    "test": "ava ./src/__tests__/**",
    "test:cov": "c8 --all --clean --include src --exclude 'src/__tests__/**' ava ./src/__tests__/** && c8 report --all --reporter json-summary --reporter lcov --include src --exclude 'src/__tests__/**'",
    "test:watch": "ava --watch"
  },
  "devDependencies": {
    "@srclaunch/dx": "workspace:*",
    "@types/diff": "^5.0.2",
    "@types/fs-extra": "^9.0.13",
    "@types/gh-pages": "^3.2.1",
    "@types/js-yaml": "^4.0.5",
    "@types/luxon": "^2.4.0",
    "@types/mocha": "^9.1.1",
    "@types/mustache": "^4.2.1",
    "@types/node": "^17.0.45",
    "@types/node-emoji": "^1.8.1",
    "@types/pluralize": "^0.0.29",
    "@types/prompts": "^2.0.14",
    "@types/react": "^17.0.2",
    "@types/rimraf": "^3.0.2",
    "@types/semantic-release": "^17.2.4",
    "@types/semver": "^7.3.10",
    "@types/standard-version": "^7.0.1",
    "@types/tape": "^4.13.2",
    "@types/update-notifier": "^5.1.0"
  },
  "dependencies": {
    "@microsoft/tsdoc": "^0.14.1",
    "@rauschma/stringio": "^1.4.0",
    "@srclaunch/ci-cd": "workspace:*",
    "@types/async": "^3.2.15",
    "@vitejs/plugin-react": "^1.3.2",
    "@yarnpkg/core": "^3.2.3",
    "ansi-styles": "^6.1.0",
    "async": "^3.2.4",
    "c8": "^7.12.0",
    "change-case": "^4.1.2",
    "chokidar": "^3.5.3",
    "css-tree": "^2.1.0",
    "diff": "^5.1.0",
    "dir-glob": "^3.0.1",
    "esbuild": "^0.14.50",
    "extract-react-types": "^0.30.2",
    "fs-extra": "^10.1.0",
    "gh-pages": "^4.0.0",
    "globby": "^13.1.2",
    "hygen": "^6.2.11",
    "ink": "^3.2.0",
    "jest": "^28.1.3",
    "js-yaml": "^4.1.0",
    "latest-version": "^6.0.0",
    "luxon": "^2.5.0",
    "meow": "^10.1.3",
    "mocha": "^10.0.0",
    "mustache": "^4.2.0",
    "nanoid": "^3.3.4",
    "ora": "^6.1.2",
    "picocolors": "^1.0.0",
    "pluralize": "^8.0.0",
    "prompts": "^2.4.2",
    "react": "^17.0.2",
    "react-docgen-typescript": "^2.2.2",
    "semantic-release": "^19.0.3",
    "semantic-release-commit-filter": "^1.0.2",
    "semantic-release-monorepo": "^7.0.5",
    "semver": "^7.3.7",
    "shelljs": "^0.8.5",
    "simple-git": "^3.10.0",
    "standard-version": "^9.5.0",
    "tape": "^5.5.3",
    "terminal-link": "^3.0.0",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "typescript": "4.6.4",
    "unconfig": "^0.3.5",
    "update-notifier": "^5.1.0",
    "vite": "^2.9.14",
    "vite-plugin-pwa": "^0.12.3",
    "vitest": "^0.14.2"
  }
}
