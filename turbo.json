{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "test": {
      "dependsOn": ["build"],
      "outputs": ["coverage/**"],
      "inputs": ["src/**/*.test.tsx", "src/**/*.test.ts"]
    },
    "release": {
      "dependsOn": ["build", "^release"],
      "outputs": []
    },
    "release:local": {
      "dependsOn": ["build", "test"],
      "outputs": []
    },
    "deploy": {
      "dependsOn": ["release"],
      "outputs": []
    },
    "deploy:local": {
      "cache": false,
      "dependsOn": ["release:local"],
      "outputs": []
    },
    "@srclaunch/cli#build": {
      "dependsOn": ["@srclaunch/ci-cd#build", "@srclaunch/types#build"],
      "outputs": []
    },
    "@srclaunch/dx#release": {
      "dependsOn": ["@srclaunch/ci-cd#release"],
      "outputs": []
    },
    "@srclaunch/types#release": {
      "dependsOn": ["@srclaunch/cli#build"],
      "outputs": []
    },
    "@srclaunch/types#release:local": {
      "dependsOn": ["@srclaunch/cli#build"],
      "outputs": []
    }
  }
}
